array myArray = [3, 2*3, 234**1];
myArray.push(4, 5);
print(myArray.has(3), myArray.has(4), myArray.has(6));
num popped = 0;
while (myArray.length) {
    popped = myArray.pop();
    if (myArray.length == 0) {
        print("All elements are gone! :(");
    } else {
        print(stringify(popped) + " was popped, " + myArray.join(", ") + " remain.");
    }
}