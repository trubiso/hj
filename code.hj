num max = input("How many numbers do you want? ");
string output = "Numbers from 1 to " + stringify(max) + ": ";
array r = range(max + 1);
array neg = r[1:];
for (number in neg) {
    output = output + stringify(number);
    if (number < max) {
        output = output + ", ";
    }
}
output = output + "! I really hope this works.";
print(output);